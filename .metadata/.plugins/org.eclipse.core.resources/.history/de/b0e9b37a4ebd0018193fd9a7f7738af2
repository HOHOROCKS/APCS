
public class Joshua{
	public static void main(String[] args){
		for (int i = 1; i <= 4; i++){
			for (int space = 4; space > 2 * i - 4; space--){
				System.out.print(" ");
			}
			System.out.print("<>");	
			for(int dot = i-1; dot <= 2 * i + 2; dot++){
				System.out.print(".");
			System.out.println("<>");
			}
		}
	}
}

