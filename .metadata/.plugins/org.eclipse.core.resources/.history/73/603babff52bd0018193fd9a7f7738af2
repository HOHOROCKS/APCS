
public class Joshua{
	public static void main(String[] args){
		for (int i = 0; i < 4; i++){
			for (int space = 4; space > 2 * i - 2; space--){
				System.out.print(" ");
			}
			
			System.out.print("<>");
			
			if (i != 0){
				for(int dot = 1; dot <= 4 * i; dot++){
					System.out.print(".");	
				}
			}
			System.out.println("<>");
		}
	}
}